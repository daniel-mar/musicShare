@model LogUser
<form action="/user/login" method="post">
    <div class="form-group mb-3">
        <label for="LogEmail">Email</label>
        <input asp-for="LogEmail" class="form-control" value="a@test.com">
        <span asp-validation-for="LogEmail" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <label for="LogPassword">Password</label>
        <input asp-for="LogPassword" class="form-control" value="123123123">
        <span asp-validation-for="LogPassword" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Login" class="btn btn-success text-white">
    </div>
</form>